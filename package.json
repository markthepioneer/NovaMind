{
  "name": "deployment-service",
  "version": "1.0.0",
  "description": "A robust and scalable deployment service that supports multiple cloud providers",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "test": "jest",
    "test:unit": "jest --testMatch='**/*.unit.test.ts'",
    "test:e2e": "jest --testMatch='**/*.e2e.test.ts'",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepare": "husky install"
  },
  "keywords": [
    "deployment",
    "kubernetes",
    "aws-lambda",
    "cloud-run",
    "microservices"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.39.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@google-cloud/monitoring": "^5.0.0",
    "@google-cloud/run": "^1.0.0",
    "@kubernetes/client-node": "^0.18.1",
    "@mui/icons-material": "^5.15.12",
    "@mui/material": "^5.17.1",
    "@reduxjs/toolkit": "^2.6.1",
    "aws-sdk": "^2.1450.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.0.0",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^7.5.0",
    "openai": "^4.90.0",
    "rate-limiter-flexible": "^3.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^9.2.0",
    "react-router-dom": "^7.4.1",
    "winston": "^3.10.0"
  },
  "devDependencies": {
    "@google-cloud/logging": "^11.2.0",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.4",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/long": "^4.0.2",
    "@types/mongodb-memory-server": "^1.8.0",
    "@types/node": "^20.5.7",
    "@types/react": "^18.2.64",
    "@types/react-dom": "^18.2.21",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^6.5.0",
    "@typescript-eslint/parser": "^6.5.0",
    "eslint": "^8.48.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^8.0.3",
    "jest": "^29.6.4",
    "lint-staged": "^14.0.1",
    "mongodb-memory-server": "^10.1.4",
    "prettier": "^3.0.3",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.2.2"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
